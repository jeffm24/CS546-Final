    <!-- Scripts we made -->
    <script>
    //For signout
    $('#signOutBtn').on('click', function(e) {

        $.ajax({
            url: '/signout',
            type: 'POST',
            success: function(data) {
                location.reload();
            },
            error: function(xhr, status, error) {
                alert(xhr.responseText + ' (' + xhr.status + ')');
            }
        });

    });
